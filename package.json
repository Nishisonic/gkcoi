    "@types/chart.js": "^2.9.29",
    "@types/chartjs-plugin-colorschemes": "^0.4.0",
    "@types/node": "^14.14.19",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "prettier": "^2.2.1",
    "ts-loader": "^8.0.13",
    "ts-node": "^9.1.1",
    "typescript": "4.0.5",
