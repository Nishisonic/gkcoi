  "devDependencies": {
    "@types/chartjs-plugin-colorschemes": "^0.4.1",
    "@types/node": "^17.0.29",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.2",
    "ts-loader": "^9.2.9",
    "ts-node": "^10.7.0",
    "typescript": "4.6.4",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
